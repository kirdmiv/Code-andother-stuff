    int n, m, x;
    vector<int > a;

    cin >> n;
    vector<int> am1(1e5+1, 0);
    vector<int> am2(1e5+1, 0);
    vector<int> us(1e5+1, 0);
    FI{
        cin >> x;
        am1[x]++;
    }
    cin >> m;
    FJ{
        cin >> x;
        a.eb(x);
    }

    int cnt = 0;
    int ans = 1;
    FI{
        am2[a[i]]++;
        if (am2[a[i]] == am1[a[i]]){
            cnt += 1;
            us[a[i]] = 1;
        }
        if (us[a[i]] && am2[a[i]] != am1[a[i]]){
            cnt --;
            us[a[i]] = 0;
        }
    }

    while (cnt != n && (ans + n-1) < m){
        cout << cn
        int i= ans + n -1;
        am2[a[i]]++;
        if (am2[a[i]] == am1[a[i]]){
            cnt += 1;
            us[a[i]] = 1;
        }
        if (us[a[i]] && am2[a[i]] != am1[a[i]]){
            cnt --;
            us[a[i]] = 0;
        }
        i = ans -1;
        am2[a[i]]--;
        if (am2[a[i]] == am1[a[i]]){
            cnt += 1;
            us[a[i]] = 1;
        }
        if (us[a[i]] && am2[a[i]] != am1[a[i]]){
            cnt --;
            us[a[i]] = 0;
        }
        ans++;
    }

